""""""""""""""""""""""""""""""""
"Simple config for wswt-aws vim
"
"Alex Jiang
""""""""""""""""""""""""""""""""
set history=1000
set autoread

set background=dark
set ruler

filetype indent plugin on
set autoindent
set si
set expandtab
set smarttab
set shiftwidth=4
set tabstop=4
set whichwrap+=<,>,[,]
set backspace=2
set tw=80

set hlsearch
set incsearch
set number
